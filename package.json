{
	"name": "facility-insights-sveltekit",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test": "npm run test:unit -- --run && npm run test:integration",
		"test:unit": "vitest run ./tests/unit",
		"test:integration": "vitest run ./tests/integration",
		"test:e2e": "playwright test",
		"updateGraphqlSchema": "genql --endpoint http://localhost:5173/api/v1/graphql --output ./src/lib/shared/generated/graphql"
	},
	"devDependencies": {
		"@iconify/svelte": "^4.0.2",
		"@playwright/test": "^1.45.3",
		"@sveltejs/adapter-node": "^5.2.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/bcrypt": "^5.0.2",
		"@types/eslint": "^9.6.0",
		"@types/jsonwebtoken": "^9.0.7",
		"@types/pg": "^8.11.10",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3",
		"vitest": "^2.0.4",
		"fluid-ui-svelte": "^0.2.4"
	},
	"dependencies": {
		"@google-cloud/storage": "^7.12.1",
		"@googlemaps/js-api-loader": "^1.16.8",
		"@sendgrid/mail": "^8.1.3",
		"@tailwindcss/vite": "^4.0.0-alpha.31",
		"bcrypt": "^5.1.1",
		"jsonwebtoken": "^9.0.2",
		"oxide.ts": "^1.1.0",
		"pg": "^8.13.1",
		"tailwindcss": "^4.0.0-alpha.31",
		"ts-postgres": "^2.0.4",
		"zod": "^3.23.8"
	}
}